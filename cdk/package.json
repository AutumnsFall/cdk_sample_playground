{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint './**/*.{js,ts}' --cache && tsc --noEmit",
    "fix": "eslint './**/*.{js,ts}' --fix --cache && tsc --noEmit",
    "synth:dev": "NODE_ENV=dev cdk synth",
    "deploy:dev": "NODE_ENV=dev cdk deploy --all"
  },
  "devDependencies": {
    "@mrgrain/cdk-esbuild": "^5.2.6",
    "@types/jest": "^29.5.12",
    "@types/node": "20.14.9",
    "aws-cdk": "2.151.0",
    "esbuild": "^0.23.1",
    "esbuild-plugin-time": "^1.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.5.3"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.7.0",
    "aws-cdk-lib": "2.151.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
